{
  "name": "alex-brukhty/craft-toolkit",
  "description": "Set of handy craft tools",
  "type": "craft-plugin",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.4.0",
    "ryssbowh/craft-warmer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "alexbrukhty\\crafttoolkit\\": "src/"
    }
  },
  "extra": {
    "handle": "_toolkit",
    "name": "Toolkit",
    "developer": "Alex Brukhty",
    "documentationUrl": "",
    "class": "alexbrukhty\\crafttoolkit\\Toolkit"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
